<?xml version="1.0" encoding="utf-8"?>
<odoo>
<template id="easy_invoice_employee_report.internal_layout">

            
    <div class="article">
        <link href="/easy_invoice_employee_report/static/src/css/report.css" rel="stylesheet"/>
      <t t-raw="0" />
    </div>

    <div class="footer">
        <div class="row">
            <div class="col-xs-6 custom_footer">
                <span t-esc="context_timestamp(datetime.datetime.now()).strftime('%Y-%m-%d %H:%M')"/>
            </div>
            <div class="col-xs-6 text-right custom_footer">
                <ul class="list-inline">
                    <li><span class="page"/></li>
                    <li>/</li>
                    <li><span class="topage"/></li>
                </ul>
            </div>
        </div>
    </div>
</template>
</odoo>

